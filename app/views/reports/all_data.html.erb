<p id="notice"><%= flash[:notice] %></p>
<p>Enter your email address below to recieve the DNA sequences<p>

<%= form_tag reports_send_email_path do %>
  <%= email_field_tag "email" %>
  <%= collection_select :assembly, :name, @assemblies, :name, :name, prompt: true %>
  <%= submit_tag "Send Email" %>
<% end %>
